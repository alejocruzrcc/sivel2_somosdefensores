
<script src='https://cdn.plot.ly/plotly-latest.min.js'></script>
<script src='https://cdn.plot.ly/plotly-locale-es-latest.js'></script>

<div class="row">
  <div class="col-sm-4">
    <div class="well" width="12" title="Filtro" status="info">
      <div id="rango" class="shiny-date-range-input form-group shiny-input-container">
        <label class="control-label" for="rango">Rango de fechas de la victimización</label>
        <div class="input-daterange input-group">
          <input class="input-sm form-control" type="text" data-date-language="en" data-date-week-start="0" data-date-format="yyyy-mm-dd" data-date-start-view="month" data-initial-date="2017-07-06" data-date-autoclose="true"/>
          <span class="input-group-addon"> to </span>
          <input class="input-sm form-control" type="text" data-date-language="en" data-date-week-start="0" data-date-format="yyyy-mm-dd" data-date-start-view="month" data-initial-date="2018-07-30" data-date-autoclose="true"/>
        </div>
      </div>
      <div class="form-group shiny-input-container">
        <label class="control-label" for="sexonac">Sexos de nacimiento</label>
        <div class="filtros">
          <select id="sexonac" class="sexoselect" multiple="multiple" >
          </select>
          <script type="application/json" data-for="sexonac">{}</script>
        </div>
      </div>
      <div class="form-group shiny-input-container">
        <label class="control-label" for="categoria_rotulo">Categorias incluidas</label>
        <div>
          <select id="categoria_rotulo" multiple="multiple"><option value="A10 EJECUCIÓN EXTRAJUDICIAL" selected>A10 EJECUCIÓN EXTRAJUDICIAL</option>
            <option value="A11 DESAPARICIÓN FORZADA" selected>A11 DESAPARICIÓN FORZADA</option>
            <option value="A12 TORTURA" selected>A12 TORTURA</option>
            <option value="A13 LESIÓN FÍSICA" selected>A13 LESIÓN FÍSICA</option></select>
          <script type="application/json" data-for="categoria_rotulo">{}</script>
        </div>
      </div>
    </div>
  </div>
  <div class="col-sm-8">
    <div class="bubbleplot" data-num="0">
      <div id="divplot" class="plot">
      </div>
    </div>
    <div id="div_serietiempo" class="plot">
    </div>
    <div id="graf_barras">
    </div>
  </div>
</div>
